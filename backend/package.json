{
  "name": "design_lab_back",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0 <20.20.0"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": " cross-env NODE_ENV=production node ./dist/main.js",
    "watch-tsc": "tsc && tsc-alias -w",
    "server:watch": "ts-node-dev --respawn --transpile-only ./src/main.ts",
    "watch-deploy": "cross-env NODE_ENV=development nodemon ./dist/main.js",
    "server": "concurrently \"yarn watch-tsc\" \"yarn watch-deploy\"",
    "lint": "eslint **/*.js",
    "lint:fix": "eslint --fix **/*.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.717.0",
    "@aws-sdk/s3-request-presigner": "^3.717.0",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.21.1",
    "gravatar": "^1.8.2",
    "jimp": "^0.22.10",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "mongoose": "^8.8.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^4.0.2",
    "nodemailer": "^7.0.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/gravatar": "^1.8.6",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "concurrently": "^9.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "nodemon": "2.0.15",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.2"
  }
}
